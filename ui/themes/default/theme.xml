	<ModelView Inherit="" Name="TemplateModelViewEditor" MouseDClick="true">
	  <Scripts>
		<OnMouseMove>
			if self:IsCaptured() then
				if uiIsKeyPressed("CTRL") then
					self:SetCameraEyeOffset(delta_y/20, 0, -delta_x/20, false);
				elseif uiIsKeyPressed("SHIFT") then
					self:MoveCamera(-delta_x/6,delta_y/6,0);
				end
			end
		</OnMouseMove>
		<OnMouseWheel>
			self:SetCameraEyeOffset(0, delta/80, 0, false);
		</OnMouseWheel>
		<OnLDClick>
			local Eye = self:GetCameraEye();
			local LookAt = self:GetCameraLookAt();
			local name = self:getName();
			self = uiGetglobal(name);
			uiEditorAttrSetAttributeEx(self, "Camera", "Eye", string.format("%.2f,%.2f,%.2f", Eye.x, Eye.y, Eye.z), false);
			self = uiGetglobal(name);
			uiEditorAttrSetAttributeEx(self, "Camera", "IsEyePosition", tostring(Eye.IsPosition), false);
			self = uiGetglobal(name);
			uiEditorAttrSetAttributeEx(self, "Camera", "LookAt", string.format("%.2f,%.2f,%.2f", LookAt.x, LookAt.y, LookAt.z), true);
		</OnLDClick>
	  </Scripts>
	</ModelView>
		
