					target = uiGetglobal(target);
					if not target then uiError("[Theme.xml:TemplateButtonResizeForm]:NoTarget error!!!"); return; end
				    local x, y = uiGetWidgetRect(self);
				    local offset = self:Get("MouseOffset");
					local _x = mouse_x - (x + offset.x);
					local _y = mouse_y - (y + offset.y);
					local _w = 0;
					local _h = 0;
					if ResizeType.Left then					_w = -_x;
					elseif ResizeType.Right then			_w =  _x;	_x = 0;
